{
  "name": "reactdomstarttemplate",
  "version": "1.0.0",
  "description": "start template reactDom",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production",
    "g": "cd .\\src\\app && create-react-file",
    "template": "mkdir .\\src\\app\\template\\ | touch .\\src\\app\\template\\template.jsx | touch .\\src\\app\\template\\template.adaptive.less | touch .\\src\\app\\template\\template.less"
  },
  "keywords": [
    "react",
    "webpack2",
    "npm",
    "bower",
    "Babel",
    "eslint",
    "jest"
  ],
  "author": "Богданов Илья <onesuch07@gmail.com>",
  "license": "MIT",
  "main": "webpack.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ONESucH/ReactDomWebpack2.git"
  },
  "bugs": {
    "url": "https://github.com/ONESucH/ReactDomWebpack2/issues"
  },
  "homepage": "https://github.com/ONESucH/ReactDomWebpack2#readme",
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.2",
    "babel-polyfill": "^6.26.0",
    "base64-inline-loader": "^1.1.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "form-serialize": "^0.7.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "less-plugin-autoprefix": "^2.0.0",
    "less-plugin-clean-css": "^1.5.1",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-router-dom": "^4.3.1",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "jest": {
    "^.+\\.jsx$": "babel-jest",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/___Testing___/fileMock.js",
      "\\.(css|less)$": "<rootDir>/___Testing___/styleMock.js"
    }
  }
}
